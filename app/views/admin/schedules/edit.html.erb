<%= form_with model: @schedule, url: admin_schedules_path, local: true do |f| %>

  <div class="title">
    <div class="form-tag">タイトル</div>
    <%= f.text_field :title %>
  </div>


  <div class="content">
    <div class="form-tag">内容</div>
    <%= f.text_field :body %>
  </div>

    <div class="time">
    <div class="form-tag">時間</div>
    <%= f.datetime_select :start_time %>
  </div>


  <div class="submit">
    <%= f.submit "編集", class: "px-3" %>
  </div>

<% end %>